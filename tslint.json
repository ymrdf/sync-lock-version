{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "jsx-wrap-multiline": false,
    "arrow-parens": false,
    "jsx-boolean-value": false,
    "jsx-alignment": false,
    "arrow-return-shorthand": [
      false
    ],
    "comment-format": [
      true,
      "check-space"
    ],
    "import-blacklist": [
      true,
      "rxjs"
    ],
    "interface-over-type-literal": false,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "max-classes-per-file": [
      false
    ],
    "max-line-length": [
      true,
      140
    ],
    "member-access": false,
    "member-ordering": [
      true,
      {
        "order": [
          "static-field",
          "instance-field",
          "constructor",
          "protected-instance-method",
          "private-instance-method"
        ]
      }
    ],
    "newline-before-return": false,
    "no-any": false,
    "no-inferrable-types": [
      true,
      "ignore-params",
      "ignore-properties"
    ],
    "no-import-side-effect": [
      true
    ],
    "no-invalid-this": [
      true,
      "check-function-in-method"
    ],
    "no-null-keyword": false,
    "no-require-imports": false,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": true,
    "no-unused-variable": [
      true,
      "react"
    ],
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "object-literal-shorthand": false,
    "one-variable-per-declaration": [
      false
    ],
    "only-arrow-functions": [
      true,
      "allow-declarations"
    ],
    "ordered-imports": [
      false
    ],
    "prefer-method-signature": false,
    "prefer-template": [
      true,
      "allow-single-concat"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "typedef": [
      true,
      "parameter",
      "property-declaration"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "allow-pascal-case",
      "allow-leading-underscore"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "always",
          "typeLiterals": "always"
        }
      }
    ],
    "no-empty-interface": false,
    "no-submodule-imports": false,
    "jsx-no-bind": false,
    "no-duplicate-imports": false
  },
  "semicolon": [
    true,
    "always"
  ]
}
